<Window x:Class="HealthCare_System.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HealthCare_System"
        mc:Ignorable="d"
        Title="Login" Height="250" Width="380">
    <Grid>
        <Button x:Name="loginBtn" Content="Login" HorizontalAlignment="Center" Margin="0,176,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.53,0.553" Height="32" Width="116" Click="loginBtn_Click"/>
        <TextBox x:Name="mailTb"  HorizontalAlignment="Center" Margin="0,29,0,0" Text="example@gmail.com" TextWrapping="Wrap" VerticalAlignment="Top" Width="312" Height="24"/>
        <PasswordBox Name="passTb" HorizontalAlignment="Center" Margin="0,93,0,0" VerticalAlignment="Top" Width="312" Height="24"/>
        <ListBox x:Name="lb" Margin="273,105,10,6"/>

    </Grid>
</Window>
