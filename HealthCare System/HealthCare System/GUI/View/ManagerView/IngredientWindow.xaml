    <Grid>
        <Label x:Name="titleLbl" Content="Label" HorizontalAlignment="Center" Margin="0,21,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="submitBtn" Content="Submit" HorizontalAlignment="Left" Margin="109,129,0,0" VerticalAlignment="Top" Height="29" Width="66" FontSize="15" FontWeight="Bold" Click="submitBtn_Click"/>
        <TextBox x:Name="nameTb" HorizontalAlignment="Left" Margin="77,83,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" Height="18" FontWeight="Bold"/>
        <Label x:Name="nameLbl" Content="Name:" HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" FontSize="17" FontWeight="Bold"/>

    </Grid>
